    { device = "/dev/disk/by-uuid/30BA-50FB";
      fsType = "vfat";
      options = [ "fmask=0077" "dmask=0077" ];
    };

  swapDevices =
    [ { device = "/dev/disk/by-uuid/3d3ce1b2-e2c4-489a-bcfc-51dce476c3de"; }
